array_filter(
                    $matches,
                    fn($match) => !is_null($match[0]),
                ),


[
                '/\s*(?<p1>[\w\-]+)?\s+(?<p2>[\w\-]+)\s+(?<p3>[\w\-]+)?\s+/',
                ['p3' => 'p3'],
                '    p1    p2    ',
                [
                    'p1' => ['p1', 4],
                    'p2' => ['p2', 10],
                    'p3' => ['p3', -1],
                ],
            ]